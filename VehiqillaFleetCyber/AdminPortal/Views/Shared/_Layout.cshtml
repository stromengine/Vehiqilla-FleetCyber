                                            <span class="align-middle">Log Out</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <!--/ User -->



                        </ul>
                    </div>


                    <!-- Search Small Screens -->
                    <div class="navbar-search-wrapper search-input-wrapper  d-none">
                        <input type="text" class="form-control search-input container-xxl border-0" placeholder="Search..." aria-label="Search...">
                        <i class="ti ti-x ti-sm search-toggler cursor-pointer"></i>
                    </div>



                </nav>

                <!-- / Navbar -->
                <!-- Content wrapper -->
                <div class="content-wrapper">

                    <!-- Content -->
                    @RenderBody()

                    <!-- / Content -->
                    <!-- Footer -->
                    <footer class="content-footer footer bg-footer-theme">
                        <div class="container-xxl">
                            <div class="footer-container d-flex align-items-center justify-content-between py-2 flex-md-row flex-column">
                                <div>
                                    COPYRIGHTS ©
                                    <script>
                                        document.write(new Date().getFullYear())
                                    </script> ALL RIGHTS RESERVED BY VEHIQILLA INC
                                </div>
                                <div>
                                </div>
                            </div>
                        </div>
                    </footer>
                    <!-- / Footer -->


                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>



        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>


        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>

    </div>
    <!-- / Layout wrapper -->
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <script src="~/Content/vendor/libs/jquery/jquery.js"></script>
    <script src="~/Content/vendor/libs/popper/popper.js"></script>
    <script src="~/Content/vendor/js/bootstrap.js"></script>
    <script src="~/Content/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Content/vendor/libs/node-waves/node-waves.js"></script>

    <script src="~/Content/vendor/libs/hammer/hammer.js"></script>
    <script src="~/Content/vendor/libs/i18n/i18n.js"></script>
    <script src="~/Content/vendor/libs/typeahead-js/typeahead.js"></script>

    <script src="~/Content/vendor/js/menu.js"></script>
    <!-- endbuild -->
    <!-- Vendors JS -->
    <script src="~/Content/vendor/libs/toastr/toastr.js"></script>
    <script src="~/Content/vendor/libs/sweetalert2/sweetalert2.js"></script>
    <script src="~/Content/vendor/libs/datatables-bs5/datatables-bootstrap5.js"></script>

    <!-- Vendors JS -->
    <!-- Main JS -->
    <script src="~/Content/js/main.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <script src="~/Scripts/axios.min.js"></script>

    <script>
        var options = {
            dom: 'fBrtip',
            buttons: [
                {
                    extend: "collection",
                    className: "btn btn-label-primary dropdown-toggle me-2",
                    text: '<i class="ti ti-file-export me-sm-1"></i> <span class="d-none d-sm-inline-block">Export</span>',
                    buttons: [
                        {
                            extend: "print",
                            text: '<i class="ti ti-printer me-1" ></i>Print',
                            className: "dropdown-item",
                            customize: function (e) {
                                $(e.document.body).css("color", config.colors.headingColor).css("border-color", config.colors.borderColor).css("background-color", config.colors.bodyBg),
                                    $(e.document.body).find("table").addClass("compact").css("color", "inherit").css("border-color", "inherit").css("background-color", "inherit");
                            },
                        },
                        {
                            extend: "csv",
                            text: '<i class="ti ti-file-text me-1" ></i>Csv',
                            className: "dropdown-item",

                        },
                        {
                            extend: "excel",
                            text: '<i class="ti ti-file-spreadsheet me-1"></i>Excel',
                            className: "dropdown-item",

                        },
                        {
                            extend: "pdf",
                            text: '<i class="ti ti-file-description me-1"></i>Pdf',
                            className: "dropdown-item",

                        },
                        {
                            extend: "copy",
                            text: '<i class="ti ti-copy me-1" ></i>Copy',
                            className: "dropdown-item",

                        },
                    ],
                },

            ],
        }
    </script>
    <!-- Page JS -->
    @RenderSection("scripts", required: false)


</body>

</html>

<!-- beautify ignore:end -->
